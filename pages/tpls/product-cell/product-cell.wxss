.commodityBox {
  margin-top: -82px;
}

.commodityList {
  border-radius: 10px;
  font-size: 14px;
  color: #454545;
  margin-bottom: 10px;
}

.commodityNum {
  padding: 10px 15px 0px 15px;
  display: flex;
  font-size: 16px;
  color: #373c3f;
  justify-content: space-between;
}

.commodityNum text {
  flex: 1;
  display: block;
}

.commodityNum view {
  flex: 1;
  text-align: right;
}

.commodityNum image {
  width: 25px;
  height: 25px;
}

.commodityInfo {
  display: flex;
  padding:10px 15px;
  justify-content: flex-start;
}

.commodityCol {
  font-size: 14px;
  flex-grow: 1;
  text-align: center;
  color: #454545;
}
.commodityCol:first-of-type{
   text-align: left;
}

.commodityCol text {
  font-size: 12px;
  color: #a2a2a2;
}
